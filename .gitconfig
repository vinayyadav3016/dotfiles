[user]
	email = vinayyadav3016@gmail.com
	name  = vinayyadav3016
[core]
	excludesfile = /home/vinay/.gitignore_global
	pager = "less -+$LESS -R"
[credential]
	helper = /usr/lib/git-core/git-credential-gnome-keyring
[pack]
	threads = 2
[alias]
	lg1  = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	lg2  = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
	lg3  = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all --simplify-by-decoration
	smdf = diff --submodule=diff
[protocol "file"]
	allow = always
